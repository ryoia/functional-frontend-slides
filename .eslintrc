{
 "ecmaFeatures": {
   "jsx": true,
   "modules": true
 },
 "env": {
   "browser": true,
   "node": true
 },
 "parser": "babel-eslint",
 "rules": {
   "quotes": [2, "single"],
   "eqeqeq": 2,
   "indent": [2, 2],
   "semi": [2, "never"],
   "no-extra-semi": 2,
   "no-var": 2,
   "strict": [2, "never"],
   "react/jsx-uses-react": 2,
   "react/jsx-uses-vars": 2,
   "react/react-in-jsx-scope": 2
 },
 "plugins": [
   "react"
 ]
}
